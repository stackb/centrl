# Configuration: Registry Base File
# -------------------------------------------------------------------

http_file = use_repo_rule("@bazel_tools//tools/build_defs/repo:http.bzl", "http_file")

# Vendor in the last known state of the registry, as a backup source of metadata
# in case we hit rate limits while fetching github repository metadata.
#
# $ bazel vendor --vendor_dir=thirdparty --repo=@registry_pb
http_file(
    name = "registry_pb",
    downloaded_file_path = "registry.pb",
    urls = [
        "https://621c132f.centrl.pages.dev/registry.pb",
    ],
)

